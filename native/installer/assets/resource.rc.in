#include <windows.h>

/* Version info */
VS_VERSION_INFO VERSIONINFO
FILEVERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
PRODUCTVERSION  @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,0
FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
FILEFLAGS       0
FILEOS          VOS__WINDOWS32
FILETYPE        VFT_APP
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName",      "Lackadaisical Security"
            VALUE "FileDescription",  "LSDAMM - Distributed AI Mesh Client"
            VALUE "FileVersion",      "@PROJECT_VERSION@"
            VALUE "InternalName",     "lsdamm-native"
            VALUE "LegalCopyright",   "(c) 2025 Lackadaisical Security"
            VALUE "OriginalFilename", "lsdamm-native.exe"
            VALUE "ProductName",      "LSDAMM"
            VALUE "ProductVersion",   "@PROJECT_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 0x04B0
    END
END

